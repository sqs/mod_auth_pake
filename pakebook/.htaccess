Options +ExecCGI
AddHandler cgi-script .py

<IfModule mod_rewrite.c>      
  RewriteEngine on
  RewriteBase /
  RewriteCond %{REQUEST_URI} !^/icons
  RewriteCond %{REQUEST_URI} !^/favicon.ico$
  RewriteCond %{REQUEST_URI} !^(/.*)+pakebook.py/
  RewriteRule ^(.*)$ pakebook.py/$1 [PT]
</IfModule>

ForceType text/html
